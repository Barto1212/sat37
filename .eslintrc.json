{
  "plugins": ["@typescript-eslint", "react"],
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/no-unescaped-entities": 0,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "prefer-const": "error",
    "object-curly-spacing": ["error", "always"]
  }
}